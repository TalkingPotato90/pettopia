{
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint 'frontend/**/*.js' --fix",
    "format": "prettier --write 'frontend/**/*.js'"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0"
  },
  "lint-staged": {
    "frontend/src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}